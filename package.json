{
  "name": "redux-starter",
  "version": "1.0.0",
  "description": "A starter React/React solution",
  "main": "server/server.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --hot --inline",
    "server": "node-inspector --web-port 8888 & node-dev server/server",
    "test": "ava --watch"
  },
  "ava": {
    "failfast": true,
    "tap": true,
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ]
    }
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "copy-webpack-plugin": "^3.0.1",
    "enzyme": "^2.3.0",
    "node-dev": "^3.1.3",
    "node-inspector": "^0.12.8",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "superagent": "^2.0.0"
  }
}
